pkgname=python-cryptography-vectors
pkgver=3.4.3
pkgrel=1
pkgdesc="Test vectors for the cryptography package"
arch=('any')
license=('Apache')
url="https://pypi.python.org/pypi/cryptography-vectors"
depends=('python')
makedepends=('python-setuptools')
source=("https://pypi.io/packages/source/c/cryptography-vectors/cryptography_vectors-$pkgver.tar.gz")
sha512sums=('6c1a13f1f4aca745383ad0ac7aca351b6e0df7bf12fb0ee4b8e9198562b480b39714f3713a5b973dd5bed0666590cd030207661c5db9037d4fc3c9a484eb10a8')

package() {
   cd cryptography_vectors-$pkgver
   python3 setup.py install --root="$pkgdir" --optimize=1
}
