sha512sums=('fede7ace8e5874abb1413b75f1caf3bd5633dbacfd5e962e4a855d857e4afc4fa51c3630fd8904149d2b1661e1a52d9a962dc6da642378d1c3d05ae64a22fde2')

package() {
