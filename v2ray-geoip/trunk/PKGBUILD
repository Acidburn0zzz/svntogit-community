source=("geoip-$pkgver.dat::https://github.com/v2fly/geoip/releases/download/$pkgver/geoip.dat")
sha512sums=('1d7fc6332243001ae4ec9e51c06b730701e1efe166914443feb96f1a2efc472f89db19f30592db7fa103cbd6887116054cbe0a936485c77d352e3de54bb652ba')

package() {
