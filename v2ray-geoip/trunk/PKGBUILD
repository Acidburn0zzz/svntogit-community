sha512sums=('4f8d73232f5375c92814a4d14b8cdb8a5c38d1970570efe6070a4d8cc5a1334cf84a35c1b0d04f1defb4a687891c51914c14a3187b30c17ee76804ce4bb14e29')

package() {
  install -Dm644 geoip-$pkgver.dat "$pkgdir"/usr/share/v2ray/geoip.dat
