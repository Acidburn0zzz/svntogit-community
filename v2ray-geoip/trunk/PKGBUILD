sha512sums=('1744798bee6e6b3060d35534b7abe172ed0e09c8e8144933baa0ca0ff1f58621d924811e81ca126cde5597f6d377ddf7511b697c06856a9c62bd5262c5c3d13d')

package() {
  install -Dm644 geoip-$pkgver.dat "$pkgdir"/usr/share/v2ray/geoip.dat
