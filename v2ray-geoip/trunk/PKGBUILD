sha512sums=('cb30546cba5ae4c3cdec9b9d61146e7528cb9c44523bb338b6bdfa2415d93d41943f09c251c9b03a2e5894f1a08a53e746ad051d302e2d229d63c490be691c44')

package() {
  install -Dm644 geoip-$pkgver.dat "$pkgdir"/usr/share/v2ray/geoip.dat
