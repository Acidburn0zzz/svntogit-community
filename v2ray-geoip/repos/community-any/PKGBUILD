sha512sums=('b3729ac50d1948dc5023549eb1fc01d618387c27da525df6eea8ab268f5ce514924f8f09a66bbd7311621ffd89201ed76214b0a35d41d5329d522709599ca365')

package() {
  install -Dm644 geoip-$pkgver.dat "$pkgdir"/usr/share/v2ray/geoip.dat
