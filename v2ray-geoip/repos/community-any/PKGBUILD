source=("geoip-$pkgver.dat::https://github.com/v2ray/geoip/releases/download/$pkgver/geoip.dat")
sha512sums=('1e58fbeae2a6c540bad4ea46a80aec64cd520b9717c92bd07759641ae38060b0453b2462efdcc4e97f0d084745610b7408cb298dd0c338071ff202c8ab46eb5f')

package() {
