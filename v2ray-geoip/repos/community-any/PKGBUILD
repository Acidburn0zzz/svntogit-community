source=("geoip-$pkgver.dat::https://github.com/v2ray/geoip/releases/download/$pkgver/geoip.dat")
sha512sums=('27686ca135f42fef5edeb22eaebd8fca6c040e49db94134a4e826bf8c55786aae7ed375b1ea7e698bc1a6ff42811cf60fbf211afb40ff8cb66c3b2278cc236af')

