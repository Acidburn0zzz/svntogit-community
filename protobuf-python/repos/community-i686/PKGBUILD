pkgname=protobuf-python
_pkgname=protobuf
pkgver=2.3.0
pkgrel=1
pkgdesc="Google's data interchange format utility (python API)."
url="http://code.google.com/p/protobuf/"
license=('Apache')
arch=('i686' 'x86_64')
source=(http://protobuf.googlecode.com/files/$_pkgname-$pkgver.tar.bz2)
groups=('devel')
depends=("python" "${_pkgname}>=${pkgver}")
makedepends=('setuptools')

build() {
  cd $srcdir/$_pkgname-$pkgver/python
  python setup.py install --prefix=/usr --root $pkgdir || return 1
}
# vim: set ft=sh ts=2 sw=2 et:
md5sums=('f2964f636b3c67d1e9d8b90819fa8ddb')
