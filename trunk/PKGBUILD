source=("geoip-$pkgver.dat::https://github.com/v2ray/geoip/releases/download/$pkgver/geoip.dat")
sha512sums=('88546b1b28230cfca1b05318875c94bb0d9bfd76578a5afb111a90eae30cf48001e82d4e7f3744b6aa82e0534560d360258041d2d2bf651ed99b489b42284641')

