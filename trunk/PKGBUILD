_pkgsourcename=libcups
pkgname=lib32-$_pkgsourcename
pkgver=1.4.2
pkgrel=5
pkgdesc="The CUPS Printing System"
arch=('x86_64')
url="http://www.cups.org/"
license=('GPL')
groups=('lib32')
depends=('lib32-gnutls' 'lib32-libtiff' 'lib32-libpng' 'lib32-avahi')
source=(http://mir.archlinux.fr/extra/os/i686/$_pkgsourcename-$pkgver-$pkgrel-i686.pkg.tar.xz)

build() {
	cd $srcdir
	mkdir -p $pkgdir/opt/lib32/usr/lib
	cp -dp usr/lib/*.so* $pkgdir/opt/lib32/usr/lib
}
md5sums=('c69cdc740bc7a8aea60a5472f438a00e')
sha256sums=('254c9a3aac27183a9375505b62cdf951f7a7842928666e6bc37fbed0f8b0592c')
