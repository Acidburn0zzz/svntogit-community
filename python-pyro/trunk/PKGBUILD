pkgname=python-pyro
pkgver=3.8.1
_realver=3.8.1
pkgrel=2
pkgdesc="Python Remote Objects"
arch=('i686' 'x86_64')
license=('MIT')
depends=('python>=2.4')
source=(http://downloads.sourceforge.net/sourceforge/pyro/Pyro-${_realver}.tar.gz)
url="http://pyro.sourceforge.net"

build() {
    cd $startdir/src/Pyro-${_realver}
    python setup.py install --root=$startdir/pkg

    install -Dm644 LICENSE ${pkgdir}/usr/share/licenses/${pkgname}/LICENSE
}
md5sums=('8ab110b43f891c9664628133753c903a')
