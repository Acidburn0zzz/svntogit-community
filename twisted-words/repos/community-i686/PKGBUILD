# Contributor: William Rea <sillywilly@gmail.com>
pkgname=twisted-words
pkgver=0.3.0
pkgrel=4
pkgdesc="Low-level protocol implementations of OSCAR,IRC,MSN,TOC and Jabber libs for python"
arch=(i686)
url="http://twistedmatrix.com/projects/words/"
license=("MIT")
depends=('twisted-web')
source=(http://tmrc.mit.edu/mirror/twisted/Words/0.3/TwistedWords-$pkgver.tar.bz2)
md5sums=('8fdb18c85bde467baddc1c7f3febb82e')

build() {
  cd $startdir/src/TwistedWords-$pkgver
  python setup.py install --root=$startdir/pkg

  install -D LICENSE $startdir/pkg/usr/share/licenses/$pkgname/LICENSE
}
