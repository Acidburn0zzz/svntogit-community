pkgname=netstat-nat
pkgver=1.4.9
pkgrel=1
pkgdesc="Small program written in C displaying NAT connections, managed by netfilter."
url="http://freshmeat.net/projects/netstat-nat/"
license=("GPL")
arch=(i686 x86_64)
depends=(iptables)
source=(http://freshmeat.net/urls/68e78e75a5b12d5408ab4f17ccaa2969)
md5sums=('45e8d0f47623f619c22a6b894cd03cbc')

build() {
  cd $startdir/src/$pkgname-$pkgver
  ./configure --prefix=/usr
  make || return 1
  make DESTDIR=$startdir/pkg install
}
