pkgname=python-memcached
pkgver=1.43
pkgrel=1
pkgdesc="A Python interface to memcached"
depends=('python')
makedepends=('setuptools')
source=(ftp://ftp.tummy.com/pub/$pkgname/$pkgname-$pkgver.tar.gz)
url="ftp://ftp.tummy.com/pub/python-memcached/"
license=('Python')
arch=('i686' 'x86_64')
build() {
  cd "$srcdir"/$pkgname-$pkgver
  python setup.py install --root=$startdir/pkg || return 1
}
md5sums=('9bfa9d9b10dfc2d91ab74486773c6fc2')
