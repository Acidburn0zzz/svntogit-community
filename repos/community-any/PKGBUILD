source=("geoip-$pkgver.dat::https://github.com/v2ray/geoip/releases/download/$pkgver/geoip.dat")
sha512sums=('6f69398e9c673a301c08dcd6cf5e6cab38694749a17196892be9c576edf2c9b33ac2d0b7ae5c1919e927103ab9b3463cb4227d0b78084c13ecdb78715ca189f8')

package() {
