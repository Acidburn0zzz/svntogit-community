pkgver=202004220001
pkgrel=1
pkgdesc="GeoIP List for V2Ray"
arch=('any')
url="https://github.com/v2ray/geoip"
license=('MIT')
source=("geoip-$pkgver.dat::https://github.com/v2ray/geoip/releases/download/$pkgver/geoip.dat")
sha512sums=('56fb17223a2343b24ad22b1bdd14e53810ce0516d3da2ba9c861fe8df47c94befaa7b0e2dfd0d8a898c43e62ec510c354e63bd9ae2c4d5a4c3b511b547fe1b38')

package() {
  install -Dm755 geoip-$pkgver.dat "$pkgdir"/usr/lib/v2ray/geoip.dat
}
