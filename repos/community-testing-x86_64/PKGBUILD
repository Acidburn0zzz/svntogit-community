_pkgsourcename=libcups
pkgname=lib32-$_pkgsourcename
pkgver=1.4.2
pkgrel=3
pkgdesc="The CUPS Printing System"
arch=('x86_64')
url="http://www.cups.org/"
license=('GPL')
groups=('lib32')
depends=('lib32-gnutls' 'lib32-libtiff' 'lib32-libpng')
source=(http://mir.archlinux.fr/extra/os/i686/$_pkgsourcename-$pkgver-$pkgrel-i686.pkg.tar.gz)

build() {
	cd $srcdir
	mkdir -p $pkgdir/opt/lib32/usr/lib
	cp -dp usr/lib/*.so* $pkgdir/opt/lib32/usr/lib
}
md5sums=('a15cf65f965f27595979cb78b199fd4a')
sha256sums=('216f22c3b8f0fd71acd388ccab36fdf7861be4a41fea6e8992bf241350c54724')
