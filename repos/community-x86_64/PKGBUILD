_pkgsourcename=readline
pkgname=lib32-$_pkgsourcename
pkgver=6.1.001
pkgrel=1
pkgdesc="GNU readline library"
arch=('x86_64')
url="ftp://ftp.cwru.edu/pub/bash/"
license=('GPL')
groups=('lib32')
depends=('lib32-glibc' 'lib32-ncurses')
source=(http://mir.archlinux.fr/core/os/i686/$_pkgsourcename-$pkgver-$pkgrel-i686.pkg.tar.gz)

build() {
	cd $srcdir
	mkdir -p $pkgdir/opt/lib32/lib
	cp -dp lib/*.so* $pkgdir/opt/lib32/lib
}
md5sums=('3f3291c5a859b3a634daff2d2e0157f5')
sha256sums=('2fe9050f139496d20071059769e5a93976391019a99cd18466ba2bdd8e037591')
